{
  "name": "@vifr/react",
  "version": "0.0.1",
  "description": "vifr react components",
  "scripts": {
    "dev": "rimraf dist && rollup -c -w",
    "build": "rimraf dist && run-s lint build-bundle build-temp-types roll-types",
    "build-bundle": "rollup -c",
    "build-temp-types": "tsc --emitDeclarationOnly",
    "roll-types": "api-extractor run && rimraf temp",
    "lint": "eslint --ext .ts,.tsx src/**",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "devDependencies": {
    "vifr": "workspace:*",
    "vite": "^2.9.5"
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "magic-string": "^0.26.1",
    "react-router-dom": "^6.2.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router": ">=6",
    "react-router-dom": ">=6"
  },
  "packageManager": "pnpm@6.32.2",
  "engines": {
    "node": ">=16"
  },
  "license": "MIT"
}
