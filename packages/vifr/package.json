{
  "name": "vifr",
  "version": "0.0.1",
  "description": "Vite-based React Framework",
  "bin": {
    "vifr": "./dist/cli.js"
  },
  "scripts": {
    "dev": "rimraf dist && rollup -c -m -w",
    "build": "rimraf dist && run-s lint build-bundle build-temp-types roll-types",
    "build-bundle": "rollup -c",
    "build-temp-types": "tsc --emitDeclarationOnly --declarationDir temp",
    "roll-types": "run-s roll-types-vifr roll-types-vifr-react roll-types-vifr-dev && rimraf temp",
    "roll-types-vifr": "api-extractor run -c api-extractor-vifr.json",
    "roll-types-vifr-react": "api-extractor run -c api-extractor-vifr-react.json",
    "roll-types-vifr-dev": "api-extractor run -c api-extractor-vifr-dev.json",
    "lint": "eslint --ext .ts,.tsx src/**",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react":{
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./dev": {
      "types": "./dist/dev/index.d.ts",
      "default": "./dist/dev/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "dist/react/index.d.ts"
      ],
      "dev": [
        "dist/dev/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.18",
    "@types/debug": "^4.1.7",
    "debug": "^4.3.3",
    "picocolors": "^1.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.17.0",
    "@vitejs/plugin-react": "^2.0.0-alpha.0",
    "cac": "^6.7.12",
    "chalk": "^5.0.0",
    "htmlparser2": "^7.2.0",
    "magic-string": "^0.26.1",
    "vite": "^3.0.0-alpha.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6.3.0"
  },
  "packageManager": "pnpm@7.1.1",
  "engines": {
    "node": ">=16"
  },
  "license": "MIT"
}
